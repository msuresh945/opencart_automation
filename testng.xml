	</listeners>

	<test thread-count="5" name="opencartTest">
		<parameter name="browser" value="chrome" />

		<groups>
			<run>
				<include name="Master" />
				<!-- <include name="Sanity" />
				<include name="Regression" /> -->
			</run>
		</groups>
		<classes>
			<class name="testCases.AccountRegistrationTest" />
			<class name="testCases.LoginTest" />
			<class name="testCases.loginDataProviderTest" />
		</classes>
	</test> <!-- opencartTest -->
</suite> <!-- opencartSuite -->
